{
  "name": "flowbite-react-admin-dashboard",
  "version": "1.1.0",
  "descrption": "flowbite-react admin dashboard",
  "homepage": "https://github.com/themesberg/flowbite-react-admin-dashboard",
  "bugs": "https://github.com/themesberg/flowbite-react-admin-dashboard/issues",
  "license": "UNLICENSED",
  "author": "Conner Davis <cdavis@scu.edu>",
  "repository": {
    "type": "git",
    "url": "https://github.com/themesberg/flowbite-react-admin-dashboard.git"
  },
  "scripts": {
    "build": "yarn typecheck && vite build",
    "dev": "vite --host --port 5174",
    "format": "prettier --check --ignore-path .gitignore --write .",
    "lint": "eslint --ignore-path .gitignore .",
    "preview": "vite preview --host",
    "typecheck": "tsc",
    "electron:dev": "concurrently --kill-others \"pnpm dev\" \"wait-on tcp:5174 && cross-env ELECTRON_START_URL=http://localhost:5174 electron .\"",
    "electron:build": "pnpm build && electron-builder",
    "electron:start": "electron ."
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8",
    "yarn": ">= 1"
  },
  "dependencies": {
    "@types/react-calendar": "^4.1.0",
    "antd": "^5.24.3",
    "antd-style": "^3.7.1",
    "apexcharts": "^3.41.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "flowbite": "^1.8.0",
    "flowbite-react": "^0.5.0",
    "lunar-typescript": "^1.7.8",
    "react-apexcharts": "^1.4.1",
    "react-calendar": "^5.1.0",
    "react-icons": "^4.10.1",
    "react-sortablejs": "^6.1.4",
    "sortablejs": "^1.15.0",
    "svgmap": "^2.10.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^24.8.8",
    "electron-builder": "^25.1.8",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0",
    "wait-on": "^8.0.2"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/strict",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:tailwindcss/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/consistent-type-imports": "error",
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off",
      "tailwindcss/classnames-order": [
        "error",
        {
          "callees": [
            "classnames"
          ],
          "config": "tailwind.config.cjs"
        }
      ],
      "tailwindcss/no-custom-classname": [
        "error",
        {
          "config": "tailwind.config.cjs"
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "main": "electron/main.js",
  "build": {
    "appId": "com.yourcompany.flowbite-react-admin",
    "productName": "Flowbite React Admin Dashboard",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.business"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office"
    }
  }
}
